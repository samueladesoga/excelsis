<section class="slice sct-color-1 bb">
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <div class="block style-7">
                    <div class="block-image">
                        <% partner1 = data.contentful.partner["3gramsNAmcmOiWeYeQEUGA"] %>
                        <% url = "http:" + partner1['photograph']['url']%>
                        <%= image_tag("#{url}", alt:"", class:"img-responsive") %>  
                    </div>
                    <div class="block-body">
                        <h3 class="block-title">
                            <span><%= partner1["firstname"]%> <%= partner1["lastname"]%></span>
                            <small><%= partner1["position"]%></small>
                        </h3>
                    </div>
                </div>
            </div>
            <div class="col-md-8">
                <p>
                    <span class="dropcap style-3 bg-base-1">L</span>
                    <%= partner1["profile"]%>
                </p>
            </div>
        </div>
    </div>
</section>

